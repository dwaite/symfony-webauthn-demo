{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-apcu": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-sodium": "*",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "fzaninotto/faker": "^1.8",
        "league/uri": "^5.3",
        "nelmio/cors-bundle": "^1.5",
        "nyholm/psr7": "^1.1",
        "php-http/curl-client": "^2.0",
        "php-http/httplug-bundle": "^1.14",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/expression-language": "4.3.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.3.*",
        "symfony/framework-bundle": "4.3.*",
        "symfony/http-client": "4.3.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "4.3.*",
        "symfony/psr-http-message-bridge": "^1.2",
        "symfony/security-bundle": "4.3.*",
        "symfony/security-csrf": "^4.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "4.3.*",
        "symfony/twig-bundle": "4.3.*",
        "symfony/validator": "4.3.*",
        "symfony/web-link": "4.3.*",
        "symfony/webpack-encore-bundle": "^1.1",
        "symfony/yaml": "4.3.*",
        "twig/extensions": "^1.5",
        "web-auth/conformance-toolset": "v2.2.x-dev",
        "web-auth/cose-lib": "v2.2.x-dev",
        "web-auth/metadata-service": "v2.2.x-dev",
        "web-auth/webauthn-lib": "v2.2.x-dev",
        "web-auth/webauthn-symfony-bundle": "v2.2.x-dev",
        "web-token/jwt-key-mgmt": "^2.0",
        "web-token/jwt-signature-algorithm-rsa": "^2.0",
        "web-token/jwt-signature-algorithm-ecdsa": "^2.0"
    },
    "require-dev": {
        "symfony/debug-pack": "^1.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "4.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "compile": [
            "php bin/console doctrine:migrations:migrate"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.3.*"
        }
    }
}
